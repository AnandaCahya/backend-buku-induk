<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet" />
</head>
<style>
    .main table, .main td, .main tr, .main th{
        border: 1px solid black;
        border-collapse: collapse;
    }
</style>
<body style="margin: 20px;">
    <div style="width: 100%; display: flex;">
        <table style="width: 50%;">
            <tr>
                <td>Nama</td>
                <td>:</td>
                <td><b>Daris Nur Rahmat</b></td>
            </tr>
            <tr>
                <td>Nomor Induk</td>
                <td>:</td>
                <td>5792/2137012</td>
            </tr>
            <tr>
                <td>Nama Sekolah</td>
                <td>:</td>
                <td><b>SMKN 2 SINGOSARI</b></td>
            </tr>
        </table>
        <table style="width: 50%;">
            <tr>
                <td>Bidang Keahlian</td>
                <td>:</td>
                <td>Teknologi Informasi</td>
            </tr>
            <tr>
                <td>Program Keahlian</td>
                <td>:</td>
                <td>Teknik Komputer dan Informatika</td>
            </tr>
            <tr>
                <td>Kompetensi Keahlian</td>
                <td>:</td>
                <td>Rekayasa Perangkat Lunak</td>
            </tr>
        </table>
    </div>
    <div class="main">
        <table style="border: 1px solid black; width: 100%;">
            <tr>
                <th rowspan="4" colspan="2">Mata Pelajaran</th>
            </tr>
            <tr>
                <th colspan="4">Tahun Pelajaran 2020/2021</th>
                <th colspan="4">Tahun Pelajaran 2021/2022</th>
                <th colspan="4">Tahun Pelajaran 2022/2023</th>
            </tr>
            <tr>
                <th colspan="2">Semester 1</th>
                <th colspan="2">Semester 2</th>
                <th colspan="2">Semester 3</th>
                <th colspan="2">Semester 4</th>
                <th colspan="2">Semester 5</th>
                <th colspan="2">Semester 6</th>
            </tr>
            <tr>
                <th>Nilai</th>
                <th>Keterangan</th>
                <th>Nilai</th>
                <th>Keterangan</th>
                <th>Nilai</th>
                <th>Keterangan</th>
                <th>Nilai</th>
                <th>Keterangan</th>
                <th>Nilai</th>
                <th>Keterangan</th>
                <th>Nilai</th>
                <th>Keterangan</th>
            </tr>


            <tr>
                <td>1.</td>
                <td>Matematika</td>
                <% Array.from({ length : 6}).forEach(function(book) { %>
                    <td>98</td>
                    <td>Sangat Bagus</td>
                <% }); %>
            </tr>

            <tr>
                <th colspan="13" style="text-align: start;">Kehadiran</th>
            </tr>

            <tr>
                <td>1.</td>
                <td>Sakit</td>
                <% Array.from({ length : 6}).forEach(function(book) { %>
                    <td colspan="2">2 Hari</td>
                <% }); %>
            </tr>
            <tr>
                <td>2.</td>
                <td>Ijin</td>
                <% Array.from({ length : 6}).forEach(function(book) { %>
                    <td colspan="2">2 Hari</td>
                <% }); %>
            </tr>
            <tr>
                <td>3.</td>
                <td>Tanpa Keterangan</td>
                <% Array.from({ length : 6}).forEach(function(book) { %>
                    <td colspan="2">2 Hari</td>
                <% }); %>
            </tr>

            
            
        </table>
    </div>
</body>
</html>